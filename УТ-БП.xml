		И Параметры.МассивСкладов.Найти(ПараметрИсточник.СкладОтправитель) = Неопределено 
		И Параметры.МассивСкладов.Найти(ПараметрИсточник.СкладПолучатель) = Неопределено Тогда
		
		Отказ = Истина;
		
	КонецЕсли;	
	
КонецЕсли;

//фильтр по БУ

Если (НЕ Отказ)
