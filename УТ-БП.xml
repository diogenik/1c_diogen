//Запрос.УстановитьПараметр("Подразделение",Параметры.ПараметрыАлгоритмов.Подразделение);
СчетаУчета=Новый СписокЗначений;
СчетаУчета.Добавить(ПланыСчетов.Хозрасчетный.НайтиПоКоду("10.01"));
СчетаУчета.Добавить(ПланыСчетов.Хозрасчетный.НайтиПоКоду("41.01"));
СчетаУчета.Добавить(ПланыСчетов.Хозрасчетный.НайтиПоКоду("43"));
Запрос.УстановитьПараметр("СчетаУчета",СчетаУчета);
